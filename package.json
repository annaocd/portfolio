{
  "name": "Portfolio",
  "version": "1.0.0",
  "engines": {
    "node": "6.0.0",
    "npm": "4.0.0"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "bem-classnames": "^1.0.7",
    "better-npm-run": "0.0.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^3.16.1",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.2",
    "express": "~4.9.8",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "plop": "^1.5.0",
    "postcss-loader": "^0.8.0",
    "progress-bar-webpack-plugin": "^1.8.0",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.3.1",
    "react-motion": "^0.4.7",
    "react-redux": "^4.0.6",
    "react-router": "~3.0.3",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^6.0.3",
    "snapsvg": "^0.4.0",
    "standard": "^7.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.0"
  },
  "babel": {
    "presets": [
      "node5",
      "react",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      },
      "production": {
        "plugins": [
          "transform-runtime",
          "transform-react-remove-prop-types"
        ]
      }
    }
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "node server.js",
    "build:server": "babel server.js -o server-compiled.js",
    "build:client": "webpack -p --config webpack.prod.config.js --progress",
    "build": "better-npm-run build",
    "clean": "rimraf project/public && rimraf server-compiled.js"
  },
  "betterScripts": {
    "build": {
      "command": "npm run clean && npm run build:client",
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}
